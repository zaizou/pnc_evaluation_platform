<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
<template id="indicator_template">
            <t t-foreach="docs" t-as="o">
                                <div t-attf-class="#{kanban_color(0)}">
                                   <t t-value="JSON.parse(o.kanban_dashboard.raw_value)" t-set="dashboard"/>
                                    <div class="container o_kanban_card_content o_visible">
                                        <div t-field="o.kanban_dashboard_graph" t-field-options='{"widget": "indicateur_dashboard_graph", "fields": ["kanban_dashboard_graph"]'></div>
                                    </div>
                                </div>
                        </t>
        </template>
</data>
        </odoo>