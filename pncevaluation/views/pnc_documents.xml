   <record model="ir.ui.view" id="axepnc_form_view">
			<field name="name">axepnc.form</field>
			<field name="model">pncevaluation.axepnc</field>
			<field name="arch" type="xml">
				<form string="Axe Formulaire">
					<sheet>
						<group>
                            <field name="name" />
							<field name="numero" />
							<field name="focus" />
							<field name="color"/>
							<field name="budgets_ids" />
